    },
    "excerpt": {
      "type": "text"
    },
    "content": {
      "type": "blocks"
    },
    "coverImage": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "meta.metadata"
    }
  }
}
